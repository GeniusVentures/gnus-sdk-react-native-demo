{
  "expo": {
    "name": "gnus-nodejs-sdk-demo",
    "slug": "gnus-nodejs-sdk-demo",
    "version": "1.0.0",
    "orientation": "portrait",
    "scheme": "myapp",
    "icon": "./assets/images/gnus-icon-white.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/gnus-logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#05090F"
    },
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "UIBackgroundModes": ["fetch"]
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/gnus-logo.png",
        "backgroundColor": "#05090F"
      },
      "package": "GNUS.AI Node.js SDK Demo",
      "permissions": ["RECEIVE_BOOT_COMPLETED", "FOREGROUND_SERVICE"],
      "plugins": ["react-native-linear-gradient"]
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": ["expo-router"],
    "experiments": {
      "typedRoutes": true
    }
  }
}
